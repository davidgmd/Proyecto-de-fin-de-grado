<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ElEscribaDelDJ.Idiomas.ES.View" 
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Escenario -->
    <system:String x:Key="ScenaryTitle">Añadir o modificar Escenario</system:String>
    <system:String x:Key="ScenaryName">Nombre de escenario</system:String>
    <system:String x:Key="ScenaryDescription">Descripción de escenario</system:String>

    <!-- Error -->
    <system:String x:Key="ErrorAlreadyExists">Este escenario ya existe, introduzca otro nombre de escenario o descripción</system:String>
    <system:String x:Key="ErrorDoesntExists">El escenario que intentas modificar, no existe</system:String>

</ResourceDictionary>